<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_data">

    <fragment
        android:id="@+id/navigation_data"
        android:name="nl.sourceassist.abilitylink.ui.home.HomeFragment"
        android:label="@string/title_data"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_help"
        android:name="nl.sourceassist.abilitylink.ui.dashboard.DashboardFragment"
        android:label="@string/title_help"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="nl.sourceassist.abilitylink.ui.notifications.NotificationsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_notifications" />
</navigation>